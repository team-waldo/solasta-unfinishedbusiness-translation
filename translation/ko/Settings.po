msgctxt "ModUi/&EnableDungeonMakerModdedContent"
msgid ""
"Enable Dungeon Maker Pro\\n"
"<i><color=#F0DAA0>[include flat rooms, 150x150 & 200x200 dungeon sizes and "
"no frills mixing assets from all environments]</color></i>"
msgstr ""
"던전 메이커 프로 활성화\\n"
"<i><color=#F0DAA0>[침실, 150x150 및 200x200 던전 크기 포함, 모든 환경의 에셋을 혼합하지 "
"않음]</color></i>"

msgctxt "ModUi/&EnableEpicPointsAndArray"
msgid ""
"Enable an epic 35 points buy system and array "
"<i><color=#F0DAA0>[17,15,13,12,10,8]</color></i> <b><i><color=#C04040E0>["
"Requires Restart]</color></i></b>"
msgstr ""
"에픽 35 포인트 분배 시스템 및 정해진 능력치 분배 "
"활성화<i><color=#F0DAA0>[17,15,13,12,10,8]</color></i> <b><i><color=#C04040E0>["
"게임 재실행 필요]</color></i></b>"

msgctxt "ModUi/&EnableFeatsAtEvenLevels"
msgid "Enable feats selection at class levels 2, 10 and 18"
msgstr "클래스 레벨 2, 10, 18에 재주 선택 활성화"

msgctxt "ModUi/&EnableFeatsAtEvenLevelsMiddle"
msgid "Enable feats selection at class levels 6 and 14"
msgstr "클래스 레벨 6, 14에 재주 선택 활성화"

msgctxt "ModUi/&EnableFighterWeaponSpecialization"
msgid ""
"Enable <color=#D89555>Fighter</color> <b>Weapon Specialization</b> at class "
"levels 8 and 16 <i><color=#F0DAA0>[get +1 to hit and to damage but "
"disadvantage with any other weapon type]</color></i>"
msgstr ""
"<color=#D89555>파이터</color> 클래스 레벨 8 과 16에서 <b>무기 특화</b> "
"활성화<i><color=#F0DAA0>[명중과 피해에 +1을 얻지만 다른 무기 유형으론 불리점을 받습니다]</color></i>"

msgctxt "ModUi/&EnableFlexibleBackgrounds"
msgid ""
"Enable flexible backgrounds <i><color=#F0DAA0>[select skill and tool "
"proficiencies from backgrounds]</color></i>"
msgstr "유연한 배경 활성화 <i><color=#F0DAA0>[배경에서 스킬 및 도구 숙련도 선택]</color></i>"

msgctxt "ModUi/&EnableFlexibleRaces"
msgid ""
"Enable flexible races <i><color=#F0DAA0>[assign ability score points instead "
"of the racial defaults]</color></i>"
msgstr "유연한 종족 활성화 <i><color=#F0DAA0>[종족 기본값 대신 능력치 점수 할당]</color></i>"

msgctxt "ModUi/&EnableHigherGroundRules"
msgid ""
"Enable higher ground rules <i><color=#F0DAA0>[+1 to hit if attacking from a "
"higher ground]</color></i>"
msgstr "고지대 규칙 활성화 <i><color=#F0DAA0>[고지대에서 공격할 경우 명중 +1]</color></i>"

msgctxt "ModUi/&EnableHotkeyDebugOverlay"
msgid ""
"Enable <color=#1E81B0>CTRL-SHIFT-(D)</color> to display the debug overlay"
msgstr "디버그 오버레이를 <color=#1E81B0>CTRL-SHIFT-(D)</color>를 눌러 활성화"

msgctxt "ModUi/&EnableHotkeyToggleHud"
msgid "Enable <color=#1E81B0>CTRL-SHIFT-(H)</color> to toggle the HUD display"
msgstr "HUD 표시를<color=#1E81B0>CTRL-SHIFT-(H)</color>로 전환 활성화"

msgctxt "ModUi/&EnableInventoryFilteringAndSorting"
msgid "Enable inventory filtering and sorting"
msgstr "인벤토리 필터링 및 정렬 활성화"

msgctxt "ModUi/&EnableInventoryTaintNonProficientItemsRed"
msgid "Tint red any item the hero isn't proficient with"
msgstr "캐릭터의 숙련이 없는 아이템은 붉은색으로 표시"

msgctxt "ModUi/&EnableInventoryTintKnownRecipesRed"
msgid "Tint known recipes red"
msgstr "알려진 제작법에 빨간색 색조 적용"

msgctxt "ModUi/&EnableInvisibleCrownOfTheMagister"
msgid "Hide the <color=#D89555>Crown of the Magister</color> on game UI"
msgstr "게임 UI에서 <color=#D89555>마지스터의 왕관</color>을 숨김"

msgctxt "ModUi/&EnableLevel20"
msgid ""
"Enable level 20 <b><i><color=#C04040E0>[Requires Restart]</color></i></b>"
msgstr "레벨 20 활성화 <b><i><color=#C04040E0>[재시작 필요]</color></i></b>"

msgctxt "ModUi/&EnableLogDialoguesToConsole"
msgid "Enable log dialogues to game console during narrative sequences"
msgstr "연속 스토리 장면 중 게임 콘솔에 대화 기록 활성화"

msgctxt "ModUi/&EnableLoggingInvalidReferencesInUserCampaigns"
msgid "Enable logging invalid references in custom campaigns"
msgstr "사용자 지정 캠페인에서 유효하지 않은 참조 로깅 활성화"

msgctxt "ModUi/&EnableMinInOutAttributes"
msgid "<i>+ Enforce ability scores minimum in & out pre-requisites</i>"
msgstr "<i>+ 최소 능력치 인/아웃 전제 조건 적용</i>"

msgctxt "ModUi/&EnableMonkAbundantKi"
msgid ""
"Enable <color=#D89555>Monk</color> <b>Abundant Ki</b> at level 2 "
"<i><color=#F0DAA0>[gain additional Ki equals to half the proficiency bonus "
"rounded up]</color></i>"
msgstr ""
"<color=#D89555>몽크</color> 2레벨에<i> <b>풍부한 기</b> 활성화<color=#F0DAA0>[숙련도 보너스의 "
"반올림에 해당하는 기를 추가로 얻습니다]</color></i>"

msgctxt "ModUi/&EnableMonkFightingStyle"
msgid ""
"Enable <color=#D89555>Monk</color> <b>Fighting Style</b> choice at level 2"
msgstr "2레벨 <color=#D89555>몽크</color>에게 <b>전투 특화</b> 선택 활성화"

msgctxt "ModUi/&EnableMonkWeaponSpecialization"
msgid ""
"Enable <color=#D89555>Monk</color> <b>Weapon Specialization</b> at class "
"levels 2 and 11 <i><color=#F0DAA0>[the selected weapon type becomes a monk "
"weapon to you]</color></i>"
msgstr ""
"<color=#D89555>몽크</color> 2 레벨과 11레벨에 <b>무기 특화</b> 활성화 <i><color=#F0DAA0>["
"선택한 무기 유형이 몽크 무기로 취급됩니다]</color></i>"

msgctxt "ModUi/&EnableMulticlass"
msgid ""
"Enable multiclass <b><i><color=#C04040E0>[Requires Restart]</color></i></b>"
msgstr "멀티클래스 활성화 <b><i><color=#C04040E0>[재시작 필요]</color></i></b>"

msgctxt "ModUi/&EnablePcgRandom"
msgid ""
"Enable a better random generator algorithm <i><color=#F0DAA0>[https://www."
"pcg-random.org]</color></i>"
msgstr ""
"더 나은 무작위 생성 알고리즘 사용 <i><color=#F0DAA0>[https://www.pcg-random."
"org]</color></i>"

msgctxt "ModUi/&EnableRejoinParty"
msgid ""
"Enable <color=#1E81B0>CTRL-SHIFT-(R)</color> to rejoin the party around the "
"selected hero or the leader if none is selected <i><color=#F0DAA0>[useful "
"with parties of 5 or 6]</color></i>"
msgstr ""
"<color=#1E81B0>CTRL-SHIFT-(R)</color>을 눌러 선택한 영웅 또는 리더를 중심으로 파티에 다시 합류 기능 "
"활성화 <i><color=#F0DAA0>[5명 또는 6명의 파티에 유용]</color></i>"

msgctxt "ModUi/&EnableRelearnSpells"
msgid ""
"<i>+ Can select cantrips or spells already learned from other classes</i>"
msgstr "<i>+ 다른 직업에서 이미 배운 캔트립이나 주문을 선택할 수 있음</i>"

msgctxt "ModUi/&EnableRespec"
msgid "Enable the RESPEC after rest action"
msgstr "휴식 후 캐릭터 재설정 가능"

msgctxt "ModUi/&EnableRogueCunningStrike"
msgid ""
"Enable <color=#D89555>Rogue</color> <b>Cunning Strike</b> and <b>Devious "
"Strike</b> at class levels 5 and 14 <i><color=#F0DAA0>[you let forgo 1 sneak "
"attack dice for additional hit effects]</color></i>"
msgstr ""
"<color=#D89555>로그</color> 5 레벨과 14 레벨에 <b>교활한 일격</b>과 <b>비열한 습격</b> "
"활성화<i><color=#F0DAA0>[추가 효과를 위해 은신 주사위를 일부 소모합니다]</color></i>"

msgctxt "ModUi/&EnableSaveByLocation"
msgid "Enable save by campaigns / locations"
msgstr "캠페인/위치별 저장 활성화"

msgctxt "ModUi/&EnableSortingDungeonMakerAssets"
msgid "Enable assets sorting on Dungeon Maker editor"
msgstr "던전 만들기 편집기에서 에셋 정렬 활성화"

msgctxt "ModUi/&EnableStatsOnHeroTooltip"
msgid ""
"Display stats on hero's tooltip <i><color=#F0DAA0>[i.e.: critical hits, "
"critical failures, etc.]</color></i>"
msgstr "영웅의 툴팁에 통계 표시 <i><color=#F0DAA0>[예: 치명타, 치명타 실패 등]</color></i>"

msgctxt "ModUi/&EnableTeleportParty"
msgid ""
"Enable <color=#1E81B0>CTRL-SHIFT-(T)</color> to teleport the party "
"<i><color=#F0DAA0>[you might break quests if you teleport to an undiscovered "
"place]</color></i>"
msgstr ""
"<color=#1E81B0>CTRL-SHIFT-(T)</color>를 눌러 파티 순간이동 활성화 <i><color=#F0DAA0>["
"발견되지 않은 장소로 순간이동하면 퀘스트가 중단될 수 있습니다]</i>"

msgctxt "ModUi/&EnableTogglesToOverwriteDefaultTestParty"
msgid ""
"Enable toggles on character pool to set your default play and test party"
msgstr "캐릭터 풀에서 토글로 기본 플레이 및 테스트 파티를 설정 활성화"

msgctxt "ModUi/&EnableTooltipDistance"
msgid ""
"Enable showing distance on tooltips when hovering over a character in combat "
"<i><color=#F0DAA0>[advised to use with the official distance calculation "
"option under rules]</color></i>"
msgstr ""
"전투 중 캐릭터 위에 마우스를 올리면 툴팁에 거리 표시 활성화 <i><color=#F0DAA0>[규칙에 따라 공식 거리 계산 옵션과 함께 "
"사용하도록 권장]</color></i>"

msgctxt "ModUi/&EnableUpcastConjureElementalAndFey"
msgid ""
"Enable upcast of <color=#D89555>Conjure Elemental</color> and <color=#"
"D89555>Conjure Fey</color>"
msgstr ""
"<color=#D89555>정령 소환 주문</color>과 <color=#D89555>페이 소환 주문</color>의 강화 시전 활성화"

msgctxt "ModUi/&EnablesAsiAndFeat"
msgid ""
"Enable both attribute scores increase and feat selection <i><color=#F0DAA0>["
"instead of an exclusive choice]</color></i>"
msgstr "능력치 증가와 재주 선택을 동시에 받기 <i><color=#F0DAA0>[기존엔 둘 중 하나만 선택 가능]</color></i>"

msgctxt "ModUi/&EncounterPercentageChance"
msgid "<color=white>Set random encounters percentage chances</color>"
msgstr "<color=white>랜덤 인카운터 확률 조정</color>"

msgctxt "ModUi/&Encounters"
msgid "Encounters"
msgstr "조우(인카운터)"

msgctxt "ModUi/&EnumerateOriginSubFeatures"
msgid ""
"Allow features from heroes to work while in <color=#D89555>Wildshape</color> "
"form"
msgstr "영웅의 특성이 <color=#D89555>변신</color> 형태일 때도 작동하도록 허용"

msgctxt "ModUi/&ExpandAll"
msgid "Expand All"
msgstr "모두 펼치기"

msgctxt "ModUi/&FactionHelp"

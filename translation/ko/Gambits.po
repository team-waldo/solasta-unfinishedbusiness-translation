msgstr ""

msgctxt "Reaction/&CustomReactionGambitParryReactDescription"
msgid "Spend a <b>gambit die</b> to reduce damage."
msgstr ""

msgctxt "Reaction/&CustomReactionGambitParryReactTitle"
msgid "React"
msgstr ""

msgctxt "Reaction/&CustomReactionGambitParryTitle"
msgid "Gambit: Parry"
msgstr ""

msgctxt "Reaction/&CustomReactionGambitPreciseDescription"
msgid ""
"{0} have missed {1} and can spend a <b>gambit die</b> to roll {3} and add "
"result to attack roll."
msgstr ""

msgctxt "Reaction/&CustomReactionGambitPreciseReactDescription"
msgid "Spend <b>gambit die</b> to add a <b>gambit die</b> roll to attack roll."
msgstr ""

msgctxt "Reaction/&CustomReactionGambitPreciseReactTitle"
msgid "Spend Die"
msgstr ""

msgctxt "Reaction/&CustomReactionGambitPreciseTitle"
msgid "Gambit: Precise"
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitBraceDescription"
msgid ""
"{0} has entered your melee reach - you can spend a <b>gambit die</b> and "
"attack it, dealing extra damage equal to the <b>gambit die</b> roll on hit."
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitBraceReactDescription"
msgid "Spend <b>gambit die</b> to attack."
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitBraceReactTitle"
msgid "Spend Die"
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitBraceTitle"
msgid "Brace"
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitReturnFireDescription"
msgid ""
"{0} missed you with their attack - you can spend a <b>gambit die</b> and "
"return fire, dealing extra damage equal to the <b>gambit die</b> roll on hit."
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitReturnFireReactDescription"
msgid "Spend <b>gambit die</b> to return fire."
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitReturnFireReactTitle"
msgid "Spend Die"
msgstr ""

msgctxt "Reaction/&ReactionAttackGambitReturnFireTitle"
msgid "Return Fire"
msgstr ""

